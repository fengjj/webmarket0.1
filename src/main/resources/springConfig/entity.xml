<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 	        
		    http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="commonAtomEntity" class="com.sitech.market.busiobject.entity.common.impl.CommonAtomEntity">
		<property name="mkCodenameDict" ref="mkCodenameDict"></property>
		<property name="commonDBO" ref="commonDBO"></property>
	</bean>
	
	<bean id="eventEntity" class="com.sitech.market.busiobject.entity.busi.impl.EventEntity" >
		<property name="event" ref="event"/>
		<property name="mkMonitorInfo" ref="mkMonitorInfo"/>
		<property name="mkMylacciInfo" ref="mkMylacciInfo"/>
	</bean>
	<bean id="userEntity" class="com.sitech.market.busiobject.entity.busi.impl.UserAtomEntity">
		<property name="mpi"  ref="mkPhoneInfo"></property>
		<property name="miqi" ref="mkIndextoqueryIdx"></property>
		<property name="uoi"  ref="mkUserordernumInfo"></property>
		<property name="mkUserInfo" ref="mkUserInfo" />
		<property name="mkChngroupDict" ref="mkChngroupDict" />
		<property name ="mkUserprodInfo" ref="mkUserprodInfo"/>
		<property name ="mkActrecordInfo" ref="mkActRecordInfo"/>
		<property name ="mkCustInfo" ref="mkCustInfo"/>
		<property name ="mkBusiordernum" ref="mkBusiordernum"/>
		<property name ="mkUserdatauseInfo" ref="mkUserdatauseInfo"/>
		<property name ="mkUsergroupmbrInfo" ref="mkUsergroupmbrInfo"/>
		<property name ="mkUseraccountoutInfo" ref="mkUseraccountoutInfo"/>
		<property name="mkActrecordtermiInfo" ref="mkActrecordtermiInfo"/>
		<property name="mkActcustInfo" ref="mkActcustInfo"/>
		<property name="mkCustPhoneInfo" ref="mkCustPhoneInfo"/>
	</bean>
	<bean id="custGroupAtomEntity" class="com.sitech.market.busiobject.entity.busi.impl.CustGroupAtomEntity">
		<property name="mkCustGroupInfo" ref="mkCustGroupInfo"></property>
		<property name="mkBusicustgroupRel" ref="mkBusicustgroupRel"></property>
		<property name="userEntity" ref="userEntity"></property>
		<property name="mkImportbusiTmp" ref="mkImportbusiTmp"></property>
		<property name="mkImportbusiexpTmp" ref="mkImportbusiexpTmp"></property>
		<property name="mkBusiContentRel" ref="mkBusiContentRel"></property>
		<property name="mkContentInfo" ref="mkContentInfo"></property>
		<property name="mkActInfo" ref="mkActInfo"></property>
	</bean>  
	<!-- 工号实体 -->
	<bean id="loginEntity" class="com.sitech.market.busiobject.entity.busi.impl.LoginEntity">
		<property name="mkLoginmsgDict" ref="mkLoginmsgDict"/>
		<property name="mkLoginpositionRel" ref="mkLoginpositionRel" /> 
	</bean>
	
	
	<!-- 客户群标签实体 -->
	<bean id="custtagInfoEntity" class="com.sitech.market.busiobject.entity.busi.impl.CusttagInfoEntity">
		<property name="mkCusttagInfo" ref="mkCusttagInfo"></property>
	</bean>
		
</beans>
