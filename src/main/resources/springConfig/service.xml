<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- ############################## 公共服务 begin #################################### -->
	 
	 <bean id="eventManagerSvc" class="com.sitech.market.atom.destribution.impl.EventManager" >
		<property name="eventEntity" ref="eventEntity"></property>
	</bean>
	
	
	<!-- 客户群服务 -->
	<bean id="atomCustGroupSvc" class="com.sitech.market.atom.destribution.impl.AtomCustGroup">
		<property name="custGroupAtomEntity" ref="custGroupAtomEntity"></property>
	</bean>
	
	<!-- ############################## 套餐配置 add by  20150804 ah #################################### -->
	
	
	
	<bean id="atomLoginSvc" class="com.sitech.market.atom.common.impl.AtomLogin">
		<property name="loginEntity" ref="loginEntity" />
	</bean>
	<bean id="atomCommonSvc" class="com.sitech.market.atom.common.impl.AtomCommon">
		<property name="commonAtomEntity" ref="commonAtomEntity" />
	</bean>
	
	<!-- 客户群标签服务 -->
	<bean id="atomCusttagSvc" class="com.sitech.market.atom.destribution.impl.AtomCusttag">
		<property name="custtagInfoEntity" ref="custtagInfoEntity"></property>
	</bean>
	
</beans>
 